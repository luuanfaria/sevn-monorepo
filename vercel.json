{
  "version": 2,
  "builds": [
    {
      "src": "packages/web",
      "use": "@vercel/next"
    },
    {
      "src": "packages/server",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/server/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "packages/web/$1"
    }
  ]
}
